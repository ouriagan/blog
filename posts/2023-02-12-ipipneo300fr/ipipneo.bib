@article{allik_mean_2017,
	title = {Mean {Profiles} of the {NEO} {Personality} {Inventory}},
	volume = {48},
	issn = {0022-0221, 1552-5422},
	url = {http://journals.sagepub.com/doi/10.1177/0022022117692100},
	doi = {10.1177/0022022117692100},
	abstract = {The Revised NEO Personality Inventory (NEO-PI-R) and its latest version, the NEO-PI-3, were designed to measure 30 distinctive personality traits, which are grouped into Neuroticism, Extraversion, Openness, Agreeableness, and Conscientiousness domains. The mean self-rated NEO-PI-R scores for 30 subscales have been reported for 36 countries or cultures in 2002. As a follow-up, this study reports the mean scores of the NEO-PI-R/3 for 71,870 participants from 76 samples and 62 different countries or cultures and 37 different languages. Mean differences in personality traits across countries and cultures were about 8.5 times smaller than differences between any two individuals randomly selected from these samples. Nevertheless, a multidimensional scaling of similarities and differences in the mean profile shape showed a clear clustering into distinctive groups of countries or cultures. This study provides further evidence that country/culture mean scores in personality are replicable and can provide reliable information about personality dispositions.},
	language = {en},
	number = {3},
	urldate = {2023-02-07},
	journal = {Journal of Cross-Cultural Psychology},
	author = {Allik, Jüri and Church, A. Timothy and Ortiz, Fernando A. and Rossier, Jérôme and Hřebíčková, Martina and de Fruyt, Filip and Realo, Anu and McCrae, Robert R.},
	month = apr,
	year = {2017},
	pages = {402--420},
	file = {Texte intégral:C\:\\Users\\maeva\\Zotero\\storage\\X9P5JM2Y\\Allik et al. - 2017 - Mean Profiles of the NEO Personality Inventory.pdf:application/pdf},
}

